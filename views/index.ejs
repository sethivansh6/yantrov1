<section id="" class="" style="background-color: white;">
  <div class="container">

    <div class="section-title">
      <br>
      <h2>Notification</h2>
      <hr style="width: 8%; opacity: 0.95;">
    </div>
 
    <div class="row" style="width: 100%;height:400px;border: 2px solid rgba(0,0, 0, 0.3); margin: 0 auto;display: flex;overflow-y: auto;overflow-x: hidden;">
      <div class="col-lg-12 col-md-12 col-sm-12" style="margin-top: 1.5%;margin-left: 1.5%;">
       <h5 style="color: #083f88;"> <i class="fa fa-newspaper" style="color: #083f88;font-weight: 700;"></i><strong> Notification/News</strong> </h5>
       <hr style="width: 100%;">
       <%not.forEach(function(not){%>
        <a href="<%=not.link%>" style="text-decoration: none;color: black;"><h6><i class="fa fa-bell" style="color: darkred;"></i> <%=not.message%></h6></a>
        <hr style="border-top: 1px dashed rgba(0,0, 0, 0.2);">
        <%})%>
    </div>
    </div>
        
          
  
  </div>
</section><!-- End Services Section -->




